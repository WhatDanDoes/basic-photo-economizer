{
  "name": "BasicPhotoEconomizer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "RN_SRC_EXT=e2e.js react-native start --config rn-cli.config.js",
    "test": "./node_modules/.bin/jest __tests__",
    "detox": "RN_SRC_EXT=e2e.js detox test -c android.emu.debug"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.5.1",
    "axios": "^0.19.0",
    "react": "^16.8.6",
    "react-native": "^0.59.9",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera.git",
    "react-native-flash-message": "^0.1.13",
    "react-native-vector-icons": "^6.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/runtime": "7.4.5",
    "babel-jest": "24.8.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "detox": "^12.11.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "metro-react-native-babel-preset": "0.54.1",
    "mocha": "^6.1.4",
    "moxios": "^0.4.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./src/setupJest.js"
    ]
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "RN_SRC_EXT=e2e.js cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5_API_28"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5_API_28"
      }
    },
    "test-runner": "jest"
  }
}
